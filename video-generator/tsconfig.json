{
    "compilerOptions": {
        "target": "esnext",
        "module": "esnext",
        "lib": [
            "dom",
            "esnext"
        ],
        "declaration": true,
        "sourceMap": true,
        "moduleResolution": "node",
        "strict": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "jsx": "react-jsx"
    },
    "include": [
        "src"
    ]
}